{
  "extends": [
    "prettier",
    "next/core-web-vitals",
    "eslint:recommended"
  ],
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.tsx"
      ],
      "parser": "@typescript-eslint/parser",
      "plugins": [
        "@typescript-eslint/eslint-plugin"
      ],
      "rules": {
        "no-unused-vars": "off",
        "no-redeclare": "off"
      }
    }
  ]
}
